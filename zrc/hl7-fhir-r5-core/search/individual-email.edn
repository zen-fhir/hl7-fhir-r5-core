{ns hl7-fhir-r5-core.search.individual-email,
 import #{zen.fhir},
 search {:zen/tags #{zen.fhir/search},
         :fhir/id "individual-email",
         :fhir/url "http://hl7.org/fhir/SearchParameter/individual-email",
         :fhir/type "token",
         :name "email",
         :expr {:Patient {:knife [["telecom" {:system "email"}]],
                          :jsonpath ["$.\"telecom\"?(@.\"system\"==\"email\")[*]"],
                          :data-types #{{:type "ContactPoint",
                                         :polymorphic? false}},
                          :template :token},
                :Person {:knife [["telecom" {:system "email"}]],
                         :jsonpath ["$.\"telecom\"?(@.\"system\"==\"email\")[*]"],
                         :data-types #{{:type "ContactPoint",
                                        :polymorphic? false}},
                         :template :token},
                :Practitioner {:knife [["telecom" {:system "email"}]],
                               :jsonpath ["$.\"telecom\"?(@.\"system\"==\"email\")[*]"],
                               :data-types #{{:type "ContactPoint",
                                              :polymorphic? false}},
                               :template :token},
                :PractitionerRole {:knife [["contact"
                                            "telecom"
                                            {:system "email"}]],
                                   :jsonpath ["$.\"contact\".\"telecom\"?(@.\"system\"==\"email\")[*]"],
                                   :data-types #{},
                                   :template :token},
                :RelatedPerson {:knife [["telecom" {:system "email"}]],
                                :jsonpath ["$.\"telecom\"?(@.\"system\"==\"email\")[*]"],
                                :data-types #{{:type "ContactPoint",
                                               :polymorphic? false}},
                                :template :token}}}}
